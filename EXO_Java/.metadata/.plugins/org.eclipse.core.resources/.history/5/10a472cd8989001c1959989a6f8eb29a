		}
		//check if askedNumber is divisible
		
		else if (askedNumber % 2 == 0) 
		
		{
			if (askedNumber != 2) 
			{
				isEven = true;
				System.out.println("your number isn't prime number");
				// check if askednumber is egal to 2
			}
				// if not > verify if askedNumber is prime number 
		}
		
		else 
		{
